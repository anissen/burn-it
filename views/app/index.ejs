<section id="sprins">
  <div class="page-header">
    <h1>Sprints <small>... like the ground is on fire!</small></h1>
  </div>

  <div class="row">
    <div class="span12">
      <table class="table table-striped table-condensed">
        <thead>
          <tr>
            <th>Sprint ID</th>
            <th>Start week</th>
            <th>Year</th>
            <th>Description</th>
          </tr>
        </thead>
        <tbody>
          <% var sprints = [
            {id: "12-16", startWeek: "16", year: "2012", description: "Implementing  PayPal"}, 
            {id: "12-18", startWeek: "18", year: "2012", description: "Completing the new front page"},
            {id: "12-20", startWeek: "20", year: "2012", description: "Refactor SRP"}
            ]; %>
          <% for (var i in sprints) { %>
          <tr>
            <td><a href="#"><i class="icon-tag"></i> <strong><%= sprints[i].id %></strong></a></td>
            <td><%= sprints[i].startWeek %></td>
            <td><%= sprints[i].year %></td>
            <td></i> <%= sprints[i].description %></td>
          </tr>
          <% } %>
        </tbody>
      </table>

      <a class="btn" data-toggle="modal" href="#myModal" >Create new sprint</a>
    </div>
  </div>
</section>

